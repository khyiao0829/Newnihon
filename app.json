{
  "expo": {
    "name": "myreactapp",
    "slug": "myreactapp",
    "scheme": "myreactapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fcf6f5"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "expo": {
      "name": "newnihon",
      "extra": {
        "API_KEY": "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",
        "DEBUG": "true"},
      "slug": "your-app-slug",
      "version": "1.0.0",
      "plugins": [
        [
          "@react-native-firebase/app",
          {
            "ios": {
              "apiKey": "YOUR_API_KEY",
              "appId": "YOUR_APP_ID"
            },
            "android": {
              "apiKey": "YOUR_API_KEY",
              "appId": "YOUR_APP_ID"
            }
          }
        ]
      ],
      "android": {
        "googleServicesFile": "path/to/google-services.json"
      },
      "ios": {
        "googleServicesFile": "path/to/GoogleService-Info.plist"
      }
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.myreactapp.myreactapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.myreactapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
